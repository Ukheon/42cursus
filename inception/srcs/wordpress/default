server  {
        listen 9000;

        server_name _;

        root /var/www/html/wordpress;
        index index.php;

        location ~ \.php$ {
                include snippets/fastcgi-php.conf;
                fastcgi_pass unix:/var/run/php/php7.3-fpm.sock;
        }
}
